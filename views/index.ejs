<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <script>
      var _questions = {};
      for(var q of __RemBookInit__.questions)
        _questions[q._id] = q;
    __RemBookInit__.questions = _questions;
    </script>
    <link rel="stylesheet" type="text/css" href="semantic/dist/semantic.min.css">
    <script src="javascripts/jquery-2.1.4.min.js"></script>
    <script src="semantic/dist/semantic.min.js"></script>
    <link rel='stylesheet' href='/stylesheets/style.css' />

    <link rel="stylesheet" type="text/css" href="BookBlock/css/bookblock.css" />
    <script src="BookBlock/js/modernizr.custom.js"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">
</head>
<body style="background-color: rgb(140,140,0)">
    <header class="ui equal height two column padded grid">
        <div id="logo" class="four wide column black ">
            <a href="#" id="title">RemBook</a>
        </div>

        <div id="search-container-container" class="twelve wide column">
            <div id="search-container" class="ui fluid left icon input big">
                <input type="text" placeholder="Search your friends...">
                <i class="search icon"></i>
            </div>
        </div>
    </header>

    <nav id="main-nav">
        <a href="#" class="selected">
            <i class="newspaper icon center aligned"></i>
            <div>Profile</div>
        </a>
        <a href="#">
            <i class="comments outline icon center aligned"></i>
            <div>Rems</div>
        </a>
    </nav>
    <div id="main">
        <div class="rem-bookblock bb-bookblock">
            <profile-page id="profile-page-mount-point"></profile-page>

        </div>

    </div><!-- /container -->

    <footer>
        Made with ‚ù§ by <span style="color:white">Delta Force</span>
    </footer>
</body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script src="BookBlock/js/jquerypp.custom.js"></script>
<script src="BookBlock/js/jquery.bookblock.js"></script>
<script src="javascripts/underscore.js"></script>
<script src="javascripts/backbone.js"></script>
<script src="javascripts/bundle.js"></script>
</html>
